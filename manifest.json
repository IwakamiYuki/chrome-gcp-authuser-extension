{
	"manifest_version": 3,
	"name": "GCP Authuser Adjuster",
	"version": "1.0",
	"permissions": [
		"tabs",
		"storage"
	],
	"content_scripts": [
		{
			"matches": ["*://console.cloud.google.com/*"],
			"js": ["content.js"]
		}
	],
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "icon16.png",
			"48": "icon48.png",
			"128": "icon128.png"
		}
	}
}
